{
    "name": "hexlet/code",
    "require": {
        "php": "^8.0",
        "slim/slim": "^4.10",
        "slim/psr7": "^1.4",
        "slim/flash": "^0.4.0",
        "php-di/php-di": "^6.0",
        "slim/php-view": "^3.4",
        "vlucas/phpdotenv": "^5.6",
        "nesbot/carbon": "^2.0",
        "illuminate/support": "^11.0",
        "imangazaliev/didom": "^2.0",
        "slim/http": "^1.4",
        "illuminate/collections": "^11.0",
        "symfony/var-dumper": "^6.0",
        "guzzlehttp/guzzle": "^7.0",
        "vlucas/valitron": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Hexlet\\Code\\": [
                "src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "PHP_CLI_SERVER_WORKERS=5 php -S 0.0.0.0:$(PORT) -t public",
        "phpstan": "vendor/bin/phpstan analyse src --level max"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^4.0",
        "codeception/module-phpbrowser": "^3.0",
        "guzzlehttp/guzzle": "^7.10",
        "phpunit/phpunit": "^12.4",
        "phpstan/phpstan": "^2.1"
    }
}
